num1 = int(input("첫번째 숫자 입력:"))
num2 = int(input("두번째 숫자 입력:"))
num3 = int(input("세번째 숫자 입력:"))
print(num1,"+",num2,"+",num3 ,"=",num1 + num2 + num3)
print(num1,"-",num2,"-",num3 ,"=",num1 - num2 - num3)
print(num1,"*",num2,"*",num3 ,"=",num1 * num2 * num3)
print(num1,"/",num2,"/",num3 ,"=",num1 / num2 / num3)
